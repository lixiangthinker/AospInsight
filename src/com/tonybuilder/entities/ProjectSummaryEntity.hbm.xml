<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.tonybuilder.entities.ProjectSummaryEntity" table="project_summary" schema="aosp_insight">
        <id name="projectSummaryId" column="project_summary_id"/>
        <property name="projectSummaryOrigId" column="project_summary_orig_id"/>
        <property name="projectSummaryAdded" column="project_summary_added"/>
        <property name="projectSummaryDeleted" column="project_summary_deleted"/>
        <property name="projectSummaryTotal" column="project_summary_total"/>
        <property name="projectSummarySince" column="project_summary_since"/>
        <property name="projectSummaryUntil" column="project_summary_until"/>
    </class>
</hibernate-mapping>