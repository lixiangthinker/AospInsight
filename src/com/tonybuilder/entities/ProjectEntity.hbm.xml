<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.tonybuilder.entities.ProjectEntity" table="project" schema="aosp_insight">
        <id name="projectId" column="project_id"/>
        <property name="projectName" column="project_name"/>
        <property name="projectTotalLines" column="project_total_lines"/>
        <property name="projectLastSubmitData" column="project_last_submit_data"/>
        <property name="projectIsExternalSrc" column="project_is_external_src"/>
        <property name="projectModuleType" column="project_module_type"/>
        <property name="projectIsDiscarded" column="project_is_discarded"/>
        <property name="projectPath" column="project_path"/>
    </class>
</hibernate-mapping>